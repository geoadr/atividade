18/
public class Algoritmo18 {
    public static void main(String[] args) {
        int x;  
        x = 10;  
        
        
        System.out.println("Valor de x = " + x);
        
      
        x = x + 1;
        
       
        System.out.println("Valor de x = " + x);
    }
}

23/
import java.util.Scanner;

public class Algoritmo23 {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        
        System.out.print("Digite número de três casas: ");
        int d = (scanner.nextInt() % 100) / 10; 
        
        System.out.println("Algarismo da casa das dezenas: " + d);
        
        scanner.close();
    }
}

25/
import java.util.Scanner;

public class Algoritmo25 {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);

        System.out.print("Digite data no formato ddmmaa: ");
        int data = scanner.nextInt();

        int dia = data / 10000;          
        int mes = (data % 10000) / 100; 
        int ano = data % 100;          

        System.out.println("\nDIA: " + dia);
        System.out.println("MES: " + mes);
        System.out.println("ANO: " + ano);

        scanner.close();
    }
}

26/
import java.util.Scanner;

public class Algoritmo26 {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);

        System.out.print("Digite data no formato DDMMAA: ");
        int data = scanner.nextInt();

        int dia = data / 10000;          
        int mes = (data % 10000) / 100;  
        int ano = data % 100;           

 
        int ndata = mes * 10000 + dia * 100 + ano;

      
        System.out.println("\nDIA: " + dia);
        System.out.println("MES: " + mes);
        System.out.println("ANO: " + ano);
        System.out.println("\nDATA NO FORMATO MMDDYY: " + ndata);

        scanner.close();
    }
}

27/
public class Algoritmo27 {
    public static void main(String[] args) {
        int x = 2, y = 5;  

        System.out.println("x = " + x);  
        System.out.println("\ny = " + y); 

        System.out.println("\nNovo valor de x = " + ++x);  
        System.out.println("Novo valor de y = " + --y);  
    }
}

