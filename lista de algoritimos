40/
import java.util.Scanner;

public class Algoritmo40 {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);

        System.out.print("Entre com o dividendo: ");
        int vali = scanner.nextInt();
        System.out.print("Entre com o divisor: ");
        int val2 = scanner.nextInt();

     
        System.out.println("\nDividendo: " + vali);
        System.out.println("Divisor: " + val2);
        System.out.println("Quociente: " + (vali / val2));
        System.out.println("Resto: " + (vali % val2));

        scanner.close();
    }
}

