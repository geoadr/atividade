11/
public class algoritimo11 {
    public static void main(String[] args) {
        int x = 2;         
        x++;             
        int y = x;        
        y--;               

        System.out.println("Aprendendo Algoritmo!!!");
        System.out.println("Valor de x: " + x);
        System.out.println("Valor de y: " + y);
    }
}

