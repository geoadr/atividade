public class Algoritmo46 {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);

        System.out.print("Digite saldo: ");
        double saldo = scanner.nextDouble();

        double nsaldo = saldo * 1.01;

        System.out.println("\nNovo saldo: " + nsaldo);

        scanner.close();  
    }
}

48/
import java.util.Scanner;

public class Algoritmo48 {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        System.out.print("Entre com o salário mínimo: ");
        double sm = sc.nextDouble();
        System.out.print("Entre com a quantidade em quilowatt: ");
        double qt = sc.nextDouble();

        double preco = sm / 700;
        double vp = preco * qt;

        System.out.println("\nPreço do quilowatt: " + preco +
                           "\nValor a ser pago: " + vp +
                           "\nValor com desconto: " + (vp * 0.9));
        sc.close();
    }
}
